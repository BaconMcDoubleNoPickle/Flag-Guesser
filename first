def doce(x):
  print("Your country is...")
  time.sleep(1)
  for key, value in flags.items():
    if x == value:
        print(f"{key}") 
    else: print("flag does not exist, please check flag criteria as Mr. Flag'Guesser's criteria for a value in a flag may be different than yours.")

      
def doce5():
  print("Is there a weapon in the flag?")
  y = input()
  if y == "yes":
    game_list.append(1)
    game_list.append(0)
  if y == "no":
    game_list.append(0)
    game_list.append(0)
  else:
    print("Please answer with a yes or no.")
    time.sleep(2)
    doce5()
  game_tuple = ()
  y = list(game_tuple)
  y.extend(game_list)
  game_tuple1 = tuple(y)
  doce(game_tuple1)  

def doce4():
  print("Is there a motto in the flag?")
  y = input()
  if y == "yes":
    game_list.append(1)
    doce5()
  if y == "no":
    game_list.append(0)
    doce5()
  else:
    print("Please answer with a yes or no.")
    time.sleep(2)
    doce4()

