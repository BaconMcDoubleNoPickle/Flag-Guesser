def primero():
  print("Answer with yes or no.")
  time.sleep(1)
  print("Is the background of the flag you are thinking of striped?")
  x = input()
  if x == "yes":
    game_list.append(1)
    tres1()
  if x == "no":
    y = [0, 0, 0, 0, 0]
    game_list.extend(y)
    tres4()
  else:
      print("Please answer with a yes or no.")
      time.sleep(2)  
      primero()
    
def balls():
  print("Type '1' when you have selected a country's flag")
  x = input() 
  if x == "1":
    primero()
    
def opening1():
  print("Hello! Welcome to the country flag guessing game (excluding Nepal \nbecause it has a stupid flag) I, Mr. Flag'Guesser am going to ask you a series of questions in order to determine what countries flag you \nare thinking of.")
  time.sleep(0)
  balls()

opening1()
